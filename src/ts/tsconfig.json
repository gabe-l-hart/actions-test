{
  "declarations": true,
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "outDir": "dist",
    "declaration": true,
    "baseUrl": ".",
    "sourceMap": true,
    "lib": [
      "dom",
      "es2017"
    ]
  },
  "include": [
    "*.ts"
  ],
  "exclude": [
    "node_modules",
    "**/*_test.ts",
    "dist"
  ]
}
